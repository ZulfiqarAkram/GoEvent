@model IEnumerable<GoEvent.Core.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Who I Follow</h1>
<hr />
<ul class="follow-container">
    @foreach (var artist in Model)
    {
        <li>
            <h3> @artist.Name</h3>
            <button class="btn btn-info btn-xs ">Following</button>

        </li>
    }
</ul>